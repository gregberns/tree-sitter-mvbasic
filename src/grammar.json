{
  "name": "the_language_name",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\\\n"
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_for"
        },
        {
          "type": "SYMBOL",
          "name": "_statement"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "declaration"
        }
      ]
    },
    "declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_expression"
            },
            {
              "type": "SYMBOL",
              "name": "method_call"
            }
          ]
        }
      ]
    },
    "_for": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "FOR"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": "TO"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "NEXT"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "method_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "bool"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Z]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_true"
        },
        {
          "type": "SYMBOL",
          "name": "_false"
        }
      ]
    },
    "_true": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "TRUE"
      }
    },
    "_false": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "FALSE"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

